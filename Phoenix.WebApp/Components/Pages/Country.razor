@page "/country"
<h3>Country</h3>
<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Code</th>
            <th>Country</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in _countries)
        {
            <tr>
                <td><a href="/countryedit/@item.Id">@item.Id</a></td>
                <td>@item.Name</td>
                <td>@item.Code</td>
                <td>@item.Name</td>
            </tr>
        }   
    </tbody>
</table>
